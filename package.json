{
  "name": "sqs-extended-ts",
  "version": "1.0.1",
  "description": "Amazon SQS Extended Client Library for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "prepare": "npm run build"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "clean": true
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "aws",
    "sqs",
    "s3",
    "extended",
    "typescript"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-sqs": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^22.15.18",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "jest": "^29.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.32.1"
  }
}
